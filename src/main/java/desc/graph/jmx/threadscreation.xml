<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE graphdesc PUBLIC "-//jrds//DTD Graph Description//EN"
	"urn:jrds:graphdesc">
<graphdesc>
	<name>JMXStartedThreads</name>
	<graphName>jmxstartedthreads-${label}</graphName>
	<graphTitle>Threads creation activity for ${label}</graphTitle>
	<verticalLabel>thread/s</verticalLabel>
	<add>
		<name>StartedThreadCount</name>
		<graphType>line</graphType>
		<legend>Thread started</legend>
	</add>
	<hosttree>
		<pathelement>HOST</pathelement>
		<pathelement>SERVICES</pathelement>
		<pathstring>JVM</pathstring>
		<pathstring>${label}</pathstring>
		<pathstring>Start threads activity</pathstring>
	</hosttree>
	<viewtree>
		<pathelement>SERVICES</pathelement>
		<pathstring>JVM</pathstring>
		<pathelement>HOST</pathelement>
		<pathstring>Start threads activity</pathstring>
		<pathstring>${label}</pathstring>
	</viewtree>
</graphdesc>
