<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE graphdesc PUBLIC "-//jrds//DTD Graph Description//EN" "urn:jrds:graphdesc">
<graphdesc>
        <name>OpenAMAuthenticationStats</name>
        <graphName>openam-auth-stats</graphName>
        <graphTitle>Authentication stats</graphTitle>
        <verticalLabel>authentication requests</verticalLabel>
        <add>
                <name>authSuccess</name>
                <graphType>line</graphType>
                <color>blue</color>
                <legend>Authentication succeeded</legend>
        </add>
        <add>
                <name>authFailure</name><reversed />
                <graphType>line</graphType>
                <color>red</color>
                <legend>Authentication failed</legend>
                
        </add>
        <hosttree>
                <pathelement>HOST</pathelement>
                <pathelement>SERVICES</pathelement>
                <pathstring>OpenAM</pathstring>
                <pathstring>Authentications operations</pathstring>
        </hosttree>
        <viewtree>
                <pathelement>SERVICES</pathelement>
                <pathstring>OpenAM</pathstring>
                <pathelement>HOST</pathelement>
                <pathstring>Authentications operations</pathstring>
        </viewtree>
</graphdesc>