<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE graphdesc PUBLIC "-//jrds//DTD Graph Description//EN"
                           "urn:jrds:graphdesc">
<graphdesc>
	<name>DhcpdPools</name>
	<graphName>dhcppools-${label}</graphName>
	<graphTitle>DHCP pool ${label} usage</graphTitle>
	<verticalLabel>IP</verticalLabel>
	<add>
		<dsName>used</dsName>
		<graphType>area</graphType>
		<color>green</color>
		<legend>IP used</legend>
	</add>
	<add>
		<dsName>free</dsName>
		<graphType>stack</graphType>
		<color>black</color>
		<legend>IP free</legend>
	</add>
	<hosttree>
		<pathstring>${host}</pathstring>
		<pathstring>Services</pathstring>
		<pathstring>DHCP</pathstring>
		<pathstring>Pool ${label}</pathstring>
	</hosttree>
	<viewtree>
		<pathstring>Services</pathstring>
		<pathstring>DHCP</pathstring>
		<pathstring>Pool ${label}</pathstring>
		<pathstring>${host}</pathstring>
	</viewtree>
</graphdesc>
