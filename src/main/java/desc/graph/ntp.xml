<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE graphdesc PUBLIC "-//jrds//DTD Graph Description//EN"
                           "urn:jrds:graphdesc">
<graphdesc>
	<name>Ntp</name>
	<graphName>ntp</graphName>
	<graphTitle>NTP status on ${host}</graphTitle>
	<unit>
		<base></base>
	</unit>
	<verticalLabel>ms</verticalLabel>
	<add>
		<name>RootDelay</name>
		<graphType>line</graphType>
		<legend>Root Delay</legend>
	</add>
	<add>
		<name>RootDispersion</name>
		<graphType>line</graphType>
		<legend>Root Dispersion</legend>
	</add>
	<add>
		<name>Offset</name>
		<graphType>line</graphType>
		<legend>Offset</legend>
	</add>
	<add>
		<name>Delay</name>
		<graphType>line</graphType>
		<legend>Delay</legend>
	</add>
	<hosttree>
		<pathelement>HOST</pathelement>
		<pathelement>SYSTEM</pathelement>
		<pathstring>ntp</pathstring>
	</hosttree>
	<viewtree>
		<pathelement>SYSTEM</pathelement>
		<pathstring>ntp</pathstring>
		<pathelement>HOST</pathelement>
	</viewtree>
</graphdesc>