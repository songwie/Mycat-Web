	<hosttree>
		<pathelement>HOST</pathelement>
		<pathelement>SERVICES</pathelement>
		<pathstring>Lighttpd Activity</pathstring>
		<pathelement>INDEX</pathelement>
		<pathstring>Servers</pathstring>
	</hosttree>
	<viewtree>
		<pathelement>SERVICES</pathelement>
		<pathstring>Lighttpd Activity</pathstring>
		<pathstring>Servers</pathstring>
