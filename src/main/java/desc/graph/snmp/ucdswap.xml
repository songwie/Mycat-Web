<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE graphdesc PUBLIC "-//jrds//DTD Graph Description//EN" "http://bidule">
<graphdesc>
  <name>Ucdswap</name>
  <graphName>ucdswap</graphName>
  <graphTitle>Virtual Memory activity on ${host}</graphTitle>
  <verticalLabel>Operation/s</verticalLabel><add>
    <name>ssRawSwapOut</name>
    
    <graphType>line</graphType>
    <color>blue</color>
    <legend>Number of pages swapped out</legend>
  </add>
  <add>
    <name>ssRawSwapIn</name>
    <reversed>true</reversed><graphType>line</graphType>
  	<color>green</color>
  	<legend>Number of pages swapped in</legend>
  </add>
  
  <hosttree>
    <pathelement>HOST</pathelement>
    <pathelement>SYSTEM</pathelement>
    <pathelement>MEMORY</pathelement>
    <pathstring>VM activity</pathstring>
  </hosttree>
  <viewtree>
    <pathelement>SYSTEM</pathelement>
    <pathelement>MEMORY</pathelement>
    <pathstring>VM activity</pathstring>
    <pathelement>HOST</pathelement>
  </viewtree>
</graphdesc>
