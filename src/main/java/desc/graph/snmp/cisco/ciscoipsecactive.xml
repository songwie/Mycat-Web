<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE graphdesc PUBLIC "-//jrds//DTD Graph Description//EN" "urn:jrds:graphdesc">
<graphdesc>
	<name>CiscoIpSecActiveTunnel</name>
	<graphName>ciscoipsecactive</graphName>
