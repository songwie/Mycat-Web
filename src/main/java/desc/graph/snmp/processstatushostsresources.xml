<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE graphdesc PUBLIC "-//jrds//DTD Graph Description//EN" "http://bidule">
<graphdesc>
	<name>processstatushostsresources</name>
	<graphName>pslist</graphName>
	<graphTitle>Number of process on ${host}</graphTitle>
	<verticalLabel>Number (cumulated)</verticalLabel>
	<add>
		<name>running</name>
		<graphType>area</graphType>
		<color>green</color>
		<legend>Running</legend>
	</add>
	<add>
		<name>runnable</name>
		<graphType>stack</graphType>
		<color>black</color>
		<legend>Runnable</legend>
	</add>
	<add>
		<name>notRunnable</name>
		<graphType>stack</graphType>
		<color>blue</color>
		<legend>notRunnable</legend>
	</add>
	<add>
		<name>invalid</name>
		<graphType>stack</graphType>
		<color>red</color>
		<legend>invalid</legend>
	</add>
	<hosttree>
		<pathelement>HOST</pathelement>
		<pathelement>SYSTEM</pathelement>
		<pathelement>LOAD</pathelement>
		<pathstring>Number of process</pathstring>
	</hosttree>
	<viewtree>
		<pathelement>SYSTEM</pathelement>
		<pathelement>LOAD</pathelement>
		<pathstring>Number of process</pathstring>
		<pathelement>HOST</pathelement>
	</viewtree>
</graphdesc>
