    <ds>
        <dsName>mtaStoredVolume</dsName>
        <dsType>gauge</dsType>
        <oid>.1.3.6.1.2.1.28.1.1.5</oid>
    </ds>
    <ds>
        <dsName>mtaTransmittedVolume</dsName>
        <dsType>counter</dsType>
        <oid>.1.3.6.1.2.1.28.1.1.6</oid>
    </ds>
    <graphs>
		<name>mtaVolume</name>
 		<name>mtaMessages</name>
		<name>mtaStoredVolume</name>
