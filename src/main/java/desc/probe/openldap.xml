<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE probedesc PUBLIC "-//jrds//DTD Probe Description//EN" "urn:jrds:probedesc">
<probedesc>
	<name>OpenLdap</name>
	<probeName>openldap</probeName>
	<probeClass>jrds.probe.Ldap</probeClass>
	<specific name="startTimePath">cn=Monitor?modifyTimestamp</specific>
	<specific name="timePattern">yyyyMMddHHmmss'Z'</specific>
	<ds>
		<dsName>base_i</dsName>
		<dsType>counter</dsType>
		<collect>cn=Operations,cn=Monitor?monitorOpInitiated</collect>
	</ds>
	<ds>
		<dsName>base_c</dsName>
		<dsType>counter</dsType>
		<collect>cn=Operations,cn=Monitor?monitorOpCompleted</collect>
	</ds>
	<ds>
		<dsName>bind_i</dsName>
		<dsType>counter</dsType>
		<collect>cn=Bind,cn=Operations,cn=Monitor?monitorOpInitiated</collect>
	</ds>
	<ds>
		<dsName>bind_c</dsName>
		<dsType>counter</dsType>
		<collect>cn=Bind,cn=Operations,cn=Monitor?monitorOpCompleted</collect>
	</ds>
	<ds>
		<dsName>unbind_i</dsName>
		<dsType>counter</dsType>
		<collect>cn=Unbind,cn=Operations,cn=Monitor?monitorOpInitiated</collect>
	</ds>
	<ds>
		<dsName>unbind_c</dsName>
		<dsType>counter</dsType>
		<collect>cn=Unbind,cn=Operations,cn=Monitor?monitorOpCompleted</collect>
	</ds>
	<ds>
		<dsName>search_i</dsName>
		<dsType>counter</dsType>
		<collect>cn=Search,cn=Operations,cn=Monitor?monitorOpInitiated</collect>
	</ds>
	<ds>
		<dsName>search_c</dsName>
		<dsType>counter</dsType>
		<collect>cn=Search,cn=Operations,cn=Monitor?monitorOpCompleted</collect>
	</ds>
	<graphs>
		<name>OpenLdap</name>
	</graphs>
</probedesc>
