<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE probedesc PUBLIC "-//jrds//DTD Probe Description//EN" "urn:jrds:probedesc">
<probedesc>
	<name>Varnish</name>
	<probeName>varnish${index}</probeName>
	<probeClass>jrds.probe.Varnish</probeClass>
	<specific name="command">stats</specific>
	<specific name="uptime">Client uptime</specific>
	<ds>
		<dsName>connrecycled</dsName>
		<dsType>counter</dsType>
		<collect>Backend conn. recycles</collect>
	</ds>
	<ds>
		<dsName>cachehits</dsName>
		<dsType>counter</dsType>
		<collect>Cache hits</collect>
	</ds>
	<ds>
		<dsName>cachemisses</dsName>
		<dsType>counter</dsType>
		<collect>Cache misses</collect>
	</ds>
	<ds>
		<dsName>bytesused</dsName>
		<dsType>gauge</dsType>
		<collect>bytes allocated</collect>
	</ds>
	<ds>
		<dsName>bytesfree</dsName>
		<dsType>gauge</dsType>
		<collect>bytes free</collect>
	</ds>
	<graphs>
		<name>VarnishCacheStats</name>
		<name>VarnishStorageStats</name>
	</graphs>
</probedesc>
