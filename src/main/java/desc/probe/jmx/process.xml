<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE probedesc PUBLIC "-//jrds//DTD Probe Description//EN" "urn:jrds:probedesc">
<probedesc>
    <name>SunJVM5Process</name>
    <probeName>jmxprocess-${connection.name}</probeName>
    <probeClass>jrds.probe.JMX</probeClass>
    <uptimefactor>1</uptimefactor>
    <ds>
        <dsName>ProcessCpuTime</dsName>
        <dsType>counter</dsType>
        <collect>java.lang:type=OperatingSystem/ProcessCpuTime</collect>
    </ds>
    <graphs>
        <name>JMXProcessCPU</name>
    </graphs>
</probedesc>
