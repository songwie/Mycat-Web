<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE probedesc PUBLIC "-//jrds//DTD Probe Description//EN" "urn:jrds:probedesc">
<probedesc>
    <name>OpenAM</name>
    <probeName>openam</probeName>
    <probeClass>jrds.probe.JMX</probeClass>
    <ds>
        <dsName>authFailure</dsName>
        <dsType>counter</dsType>
        <collect>SUN_OPENSSO_SERVER_MIB_SsoServerInstance:name=com.sun.identity.monitoring.SsoServerAuthSvc/AuthenticationFailureCount</collect>
    </ds>
    <ds>
        <dsName>authSuccess</dsName>
        <dsType>counter</dsType>
        <collect>SUN_OPENSSO_SERVER_MIB_SsoServerInstance:name=com.sun.identity.monitoring.SsoServerAuthSvc/AuthenticationSuccessCount</collect>
    </ds>
    <graphs>
        <name>OpenAMAuthenticationStats</name>
    </graphs>
</probedesc>
