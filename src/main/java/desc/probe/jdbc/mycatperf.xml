	<specific name="sumrows">true</specific>
	<ds>
		<dsName>NET_IN</dsName>
		<dsType>Counter</dsType>
	</ds>
	<ds>
		<dsName>NET_OUT</dsName>
		<dsType>Counter</dsType>
	</ds>
	<ds>
		<dsName>FC_COUNT</dsName>
		<dsType>Gauge</dsType>
	</ds>
	<ds>
		<dsName>BC_COUNT</dsName>
		<dsType>Gauge</dsType>
	</ds>
	<graphs>
		<name>MycatFluxGraph</name>
		<name>MycatConnectionGraph</name>
		
	</graphs>
</probedesc>


